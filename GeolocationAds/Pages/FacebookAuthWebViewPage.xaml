<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="GeolocationAds.Pages.FacebookAuthWebViewPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Sign in with Facebook"
    Routing.Route="FacebookAuthWebViewPage">


    <WebView
        x:Name="FacebookWebView"
        Navigating="OnNavigating"
        Source="{Binding FacebookAuthUrl, Mode=TwoWay}" />

</ContentPage>